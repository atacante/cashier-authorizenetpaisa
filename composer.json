{
    "name": "atacante/cashier-authorizenetpaisa",
    "description": "Laravel Cashier-Authorize provides an expressive, fluent interface to Authorize.net's subscription billing services.",
    "keywords": ["laravel", "stripe", "billing","authorize.net","payment module"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shahid Karimi",
            "email": "karimi.shahid@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/goetas/serializer.git"
        }
    ],
        "require": {
            "authorizenet/authorizenet": "dev-master",
            "dompdf/dompdf": "^0.6.1",
            "illuminate/container": "^5.2|^5.1",
            "illuminate/database": "~5.1",
            "illuminate/support": "~5.1",
            "nesbot/carbon": "~1.0",
            "php": ">=5.5.9",
            "symfony/http-kernel": "~2.7|~3.0"
        },
        "require-dev": {
            "goetas/xsd-reader": "2.*@dev",
            "goetas/xsd2php": "2.0.x-dev#fdc2ab0bb3f2b3ab796ca567cf8c0f3446a7ea3a",
            "illuminate/http": "~5.1",
            "illuminate/routing": "~5.1",
            "illuminate/view": "~5.1",
            "mockery/mockery": "~0.9",
            "phpunit/phpunit": "~4.0",
            "vlucas/phpdotenv": "~2.0"
        },
    "autoload": {
        "psr-4": {
            "Laravel\\CashierAuthorizeNet\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0-dev"
        }
    }
}
